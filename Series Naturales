#include "stdafx.h"
#include <iostream>
#include "conio.h"

using namespace std;

void main()
{
int suma,i,N;
cout<< "Ingresar el valor de N: ";
cin >> N;
suma=0;
for (i=1;i<=N;i++)
    suma=suma+(i*i);
cout<< "la Suma de los "<< N <<" de los primeros cuadrados naturales es: " << suma;
getch();
}
